{
  "name": "habitfract-monorepo",
  "version": "0.1.0-alpha.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "AGENTS=1 pnpm network",
    "dev": "dotenv -- concurrently \"npm run start:backend-services\" \"npm run start:backend-dev\" \"VITE_NODE_ENV=dev npm run start:frontend\" \"cd app && npm run generate:watch\" \"npm run open-browser\"",
    "test": "npm run test:zomes; npm run test:ui",
    "test:ui": "node node_modules/jest/bin/jest.js --config jest.config.js --silent=true",
    "test:ui-watch-current": "node node_modules/jest/bin/jest.js -t CURRENT --watch --config jest.config.js ",
    "build:frontend": "cd ui && npm run generate && npm run build",
    "start:frontend": "cd ui && npm run start",
    "open-browser": "sleep 15 && open-cli http://localhost:${VITE_UI_PORT}",
    "build:backend": "cd crates && npm run build",
    "start:backend-services": "cd crates && npm run start:precheck ",
    "start:backend-dev": "cd crates && npm run start:dev",
    "start:backend-prod": "cd crates && npm run start:prod",
    "test:zomes": "cd tests && npm run test",
    "check-env": "test ! -f .env && cp .env.example .env && cp .env crates/.env; exit 0",
    "network": "pnpm build:happ && BOOTSTRAP_PORT=$(port) SIGNAL_PORT=$(port) INTERNAL_IP=$(internal-ip --ipv4) concurrently -k \"pnpm local-services\" \"VITE_NODE_ENV=tauri pnpm run start:frontend\" \"pnpm launch\"",
    "build:happ": "npm run build:zomes && hc app pack workdir --recursive",
    "build:zomes": "CARGO_TARGET_DIR=target cargo build --release --target wasm32-unknown-unknown --workspace --exclude habit_fract",
    "local-services": "hc run-local-services --bootstrap-interface $INTERNAL_IP --bootstrap-port $BOOTSTRAP_PORT --signal-interfaces $INTERNAL_IP --signal-port $SIGNAL_PORT",
    "network:android": "pnpm build:happ && BOOTSTRAP_PORT=$(port) SIGNAL_PORT=$(port) INTERNAL_IP=$(internal-ip --ipv4) concurrently -k \"pnpm local-services\" \"VITE_NODE_ENV=tauri pnpm run start:frontend\" \"pnpm tauri dev\" \"pnpm tauri android dev\"",
    "launch": "concurrently-repeat \"pnpm run tauri dev\" $AGENTS",
    "tauri": "tauri"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@tauri-apps/api": "2.0.0-beta.13",
    "@tauri-apps/plugin-updater": "2.0.0-beta.7",
    "antd": "^5.11.0",
    "apexcharts": "^3.44.0",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "d3-array": "^3.2.4",
    "d3-ease": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-svg-legend": "^2.25.6",
    "d3-zoom": "^3.0.0",
    "flowbite": "^2.0.0",
    "flowbite-react": "^0.6.4",
    "formik": "^2.4.5",
    "graphql-tag": "^2.12.6",
    "habit-fract-design-system": "workspace:*",
    "jotai": "^2.5.1",
    "jotai-minidb": "^0.0.7",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.6",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@dicebear/collection": "^5.3.5",
    "@dicebear/core": "^5.3.5",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-tools/utils": "^10.1.0",
    "@holochain-playground/cli": "^0.2.0",
    "@holochain/hc-spin": "^0.200.10",
    "@jest/globals": "^29.7.0",
    "@playlyfe/gql": "^2.6.2",
    "@rollup/plugin-graphql": "^2.0.4",
    "@tauri-apps/cli": "^2.0.0-beta.13",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.37",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-import": "^1.13.8",
    "concurrently": "^8.2.2",
    "concurrently-repeat": "^0.0.1",
    "dotenv-cli": "^7.3.0",
    "fake-indexeddb": "^5.0.2",
    "internal-ip": "^7.0.0",
