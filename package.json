    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-graphql": "^2.1.0",
    "new-port-cli": "^1.0.0",
    "open-cli": "^7.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.5",
    "vite-plugin-node-polyfills": "^0.15.0"
  }
}