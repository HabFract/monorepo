{
  "name": "ui",
  "private": true,
  "version": "0.1.0-alpha.1",
  "type": "module",
  "scripts": {
    "start": "vite --clearScreen false",
    "build": "vite build && npm run zip",
    "generate": "graphql-codegen --config codegen.ts",
    "generate:watch": "graphql-codegen --watch --config codegen.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "zip": "cd ./dist && bestzip ../../ui.zip *",
    "clean": "rimraf ui.zip dist"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@graphql-tools/schema": "^10.0.2",
    "@holochain-open-dev/utils": "^0.16.3",
    "@holochain/client": "^0.17.0-rc",
    "deep-for-each": "^3.0.0",
    "fecha": "^4.2.3",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "hammerjs": "^2.0.8",
    "is-object": "^1.0.2",
    "js-base64": "^3.7.5",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "propagating-hammerjs": "^2.0.1",
    "habit-fract-design-system": "workspace:*",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@msgpack/msgpack": "3.0.0-beta2",
    "@parcel/watcher": "^2.3.0",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-import": "^1.13.8",
    "bestzip": "^2.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "graphql-codegen-typescript-mock-data": "^3.7.1",
    "rc-picker": "4.0.0-alpha.9",
    "vite": "^4.5.0"
  }
}